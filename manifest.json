{
    "name": "Pomopod",
    "version": "1.0.0",
    "description": "A pomodoro time management assistant with ambient noise",
    "manifest_version": 2,
    "author": "Nivedita Prabhu",

    "icons":{"128":"/icon/clockRunning.png"},

    "browser_action": {
         "default_icon": "/icon/clockRunning.png",
         "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["popup.js"],
        "persistent": false
    }

}